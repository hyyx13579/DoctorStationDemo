<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/insp_report_detais_analysis_ly"
    android:layout_width="match_parent"
    android:layout_height="45dp"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/id_insp_report_detais_analysis_lv_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="序号"
        android:textColor="#505050"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/name_insp_report_detais_analysis_lv_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="检测项目"
        android:textColor="#505050"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/vlaue_insp_report_detais_analysis_lv_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="参考值"
        android:textColor="#505050"
        android:textSize="14sp" />


</LinearLayout>