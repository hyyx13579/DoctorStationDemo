<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/compare_item_layout_h"
    android:orientation="horizontal"
    android:paddingBottom="10dp"
    android:paddingTop="10dp">

    <TextView
        android:textSize="10sp"
        android:id="@+id/report_compare_name_txt"
        android:layout_width="20dp"
        android:layout_height="@dimen/compare_view_h"
        android:gravity="center" />

    <com.founder.ihospital_bdrm_doctor_station_offline.View.CompareView.CompareView
        android:id="@+id/compare_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/compare_view_h"
        android:layout_marginLeft="10dp" />

</LinearLayout>